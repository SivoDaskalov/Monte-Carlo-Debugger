<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<simulation>
    <configuration>
        <title>Test simulation</title>
        <description>This configuration exists to test the marshaling and unmarshaling of requests</description>
        <simulationRuns>10</simulationRuns>
    </configuration>
    <variables>
        <gaussian id="X0">
            <mean>5.0</mean>
            <deviation>0.3333333333333333</deviation>
        </gaussian>
        <uniform id="Y0">
            <lowerBound>0.0</lowerBound>
            <upperBound>10.0</upperBound>
        </uniform>
        <exponential id="Z0">
            <lambda>1.0</lambda>
        </exponential>
        <gaussian id="X1">
            <mean>15.0</mean>
            <deviation>0.3333333333333333</deviation>
        </gaussian>
        <uniform id="Y1">
            <lowerBound>10.0</lowerBound>
            <upperBound>20.0</upperBound>
        </uniform>
        <exponential id="Z1">
            <lambda>1.0</lambda>
        </exponential>
        <gaussian id="X2">
            <mean>25.0</mean>
            <deviation>0.3333333333333333</deviation>
        </gaussian>
        <uniform id="Y2">
            <lowerBound>20.0</lowerBound>
            <upperBound>30.0</upperBound>
        </uniform>
        <exponential id="Z2">
            <lambda>1.0</lambda>
        </exponential>
    </variables>
    <formula>
        <sum nodeId="76">
            <product nodeId="77">
                <variable name="X0" nodeId="78"/>
                <variable name="Y0" nodeId="79"/>
                <exponentiation nodeId="82">
                    <arguments>
                        <variable name="Z0" nodeId="80" role="base"/>
                        <constant value="2.0" nodeId="81" role="exponent"/>
                    </arguments>
                </exponentiation>
            </product>
            <product nodeId="83">
                <variable name="X1" nodeId="84"/>
                <variable name="Y1" nodeId="85"/>
                <exponentiation nodeId="88">
                    <arguments>
                        <variable name="Z1" nodeId="86" role="base"/>
                        <constant value="2.0" nodeId="87" role="exponent"/>
                    </arguments>
                </exponentiation>
            </product>
            <product nodeId="89">
                <variable name="X2" nodeId="90"/>
                <variable name="Y2" nodeId="91"/>
                <exponentiation nodeId="94">
                    <arguments>
                        <variable name="Z2" nodeId="92" role="base"/>
                        <constant value="2.0" nodeId="93" role="exponent"/>
                    </arguments>
                </exponentiation>
            </product>
        </sum>
    </formula>
    <values/>
</simulation>
