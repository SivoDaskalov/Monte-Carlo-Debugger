<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<simulation>
    <properties>
        <title>Test simulation</title>
        <description>This configuration exists to test the marshaling and unmarshaling of requests</description>
        <simulationRuns>1000</simulationRuns>
    </properties>
    <variables>
        <gaussian id="X0">
            <mean>5.0</mean>
            <deviation>0.3333333333333333</deviation>
        </gaussian>
        <uniform id="Y0">
            <lowerBound>0.0</lowerBound>
            <upperBound>10.0</upperBound>
        </uniform>
        <exponential id="Z0">
            <lambda>1.0</lambda>
        </exponential>
        <gaussian id="X1">
            <mean>15.0</mean>
            <deviation>0.3333333333333333</deviation>
        </gaussian>
        <uniform id="Y1">
            <lowerBound>10.0</lowerBound>
            <upperBound>20.0</upperBound>
        </uniform>
        <exponential id="Z1">
            <lambda>1.0</lambda>
        </exponential>
        <gaussian id="X2">
            <mean>25.0</mean>
            <deviation>0.3333333333333333</deviation>
        </gaussian>
        <uniform id="Y2">
            <lowerBound>20.0</lowerBound>
            <upperBound>30.0</upperBound>
        </uniform>
        <exponential id="Z2">
            <lambda>1.0</lambda>
        </exponential>
    </variables>
    <formula>
        <sum nodeId="0" description="This is the sum of all the trucks I give" index="0">
            <product nodeId="1" index="1">
                <variable name="X0" nodeId="2" index="2"/>
                <variable name="Y0" nodeId="3" index="3"/>
                <exponentiation nodeId="6" index="4">
                    <arguments>
                        <variable name="Z0" nodeId="4" role="base" index="5"/>
                        <constant value="2.0" nodeId="5" role="exponent" index="6"/>
                    </arguments>
                </exponentiation>
            </product>
            <product nodeId="7" index="7">
                <variable name="X1" nodeId="8" index="8"/>
                <variable name="Y1" nodeId="9" index="9"/>
                <exponentiation nodeId="12" index="10">
                    <arguments>
                        <variable name="Z1" nodeId="10" role="base" index="11"/>
                        <constant value="2.0" nodeId="11" role="exponent" index="12"/>
                    </arguments>
                </exponentiation>
            </product>
            <product nodeId="13" index="13">
                <variable name="X2" nodeId="14" index="14"/>
                <variable name="Y2" nodeId="15" index="15"/>
                <exponentiation nodeId="18" index="16">
                    <arguments>
                        <variable name="Z2" nodeId="16" role="base" index="17"/>
                        <constant value="2.0" nodeId="17" role="exponent" index="18"/>
                    </arguments>
                </exponentiation>
            </product>
        </sum>
    </formula>
</simulation>
